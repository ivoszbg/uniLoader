config APPLE_SILICON
	bool
	default n

config EXYNOS
	bool
	default n

config MEDIATEK
	bool
	default n

config QUALCOMM
	bool
	default n

 choice
	prompt "SoC Support"

	config APPLE_T7000
		bool "Support for Apple T7000"
		default n
		select APPLE_SILICON
		help
